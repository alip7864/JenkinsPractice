properties([parameters([choice(choices: 'master \n feature', description: 'please select the branch', name: 'branch_name')])])
pipeline {
	agent any 
		stages {
			stage('Git_Checkout'){
				steps{ 
					echo "pulling changes from branch ${params.branch_name}"
					git url: 'https://github.com/alip7864/JenkinsPractice.git', branch: "${params.branch_name}"
				}
			}

			stage('Build'){
				steps{
					bat label: '', script: 'Build.bat'
				}
			}
			
			stage('Unit test'){
				steps{
					bat label: '', script: 'Junit.bat'
				}
			}
			stage('Quality test'){
				steps{
					bat label: '', script: 'Qualiy.bat'
				}
			}
			stage('Deploy'){
				steps{
					bat label: '', script: 'Deploy.bat'
				}
			}
			
		}
		post{
			always {
				echo "This will run always";
		
			}
			success {
				echo 'this will run only if successfull';
			}
			failure {
				echo 'This will run only if failure';
			}
			unstable {
				echo 'This will run only if run is marked as unstable';
			}
			changed {
				echo 'This will run only if the state of the pipeline has changed';
			}
		}
	
}